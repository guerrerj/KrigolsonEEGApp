{
  "hosting": [
    {
    "target":"EEGApp",
    "public": "dist/EEGApp",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
],
  "remoteconfig": {
    "template": "remoteconfig.template.json"
  }
}
